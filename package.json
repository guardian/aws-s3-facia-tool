{
  "name": "aws-s3-facia-tool",
  "description": "Iterate on S3 buckets in facia tool",
  "version": "1.2.0",
  "main": "dist/bundle.js",
  "scripts": {
    "lint": "eslint lib",
    "pretest": "npm run-script lint",
    "test": "babel-node ./node_modules/istanbul/lib/cli.js cover node_modules/mocha/bin/_mocha test",
    "posttest": "istanbul check-coverage --statements 85 --branches 70 --functions 100 --lines 90",
    "prepublish": "npm run bundle && npm test",
    "watchTests": "mocha test -w --compilers js:babel-register",
    "bundle": "rollup -c"
  },
  "publishConfig": {
    "registry": "http://registry.npmjs.org"
  },
  "license": "MIT",
  "devDependencies": {
    "async": "^1.5.2",
    "aws-sdk": "^2.3.5",
    "babel-preset-es2015": "^6.6.0",
    "babel-register": "^6.7.2",
    "chai": "^3.5.0",
    "chai-as-promised": "^5.3.0",
    "eslint": "^2.8.0",
    "istanbul": "^0.4.3",
    "lodash": "^4.11.1",
    "mkdirp": "^0.5.1",
    "mocha": "^2.4.5",
    "moment": "^2.13.0",
    "rollup": "^0.25.8",
    "rollup-plugin-commonjs": "^2.2.1",
    "rollup-plugin-json": "^2.0.0",
    "rollup-plugin-node-resolve": "^1.5.0",
    "sift": "^3.2.1"
  },
  "peerDependencies": {
    "aws-sdk": "2.3.x"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:guardian/aws-s3-facia-tool.git"
  },
  "files": [
    "dist"
  ]
}

{
  "name": "aws-s3-facia-tool",
  "description": "Iterate on S3 buckets in facia tool",
  "version": "1.0.0",
  "main": "dist/bundle.js",
  "scripts": {
    "lint": "eslint lib",
    "pretest": "npm run-script lint",
    "test": "istanbul cover ./node_modules/.bin/_mocha test",
    "posttest": "istanbul check-coverage --statements 85 --branches 70 --functions 100 --lines 90",
    "prepublish": "npm test && npm prune",
    "watchTests": "mocha test -w",
    "bundle": "rollup -c",
    "prepublish": "npm run bundle && npm test"
  },
  "publishConfig": {
    "registry": "http://registry.npmjs.org"
  },
  "license": "MIT",
  "devDependencies": {
    "async": "^1.5.2",
    "aws-sdk": "^2.2.48",
    "chai": "^3.5.0",
    "es6-promise": "^3.1.2",
    "eslint": "^2.5.3",
    "istanbul": "^0.4.2",
    "lodash": "^4.6.1",
    "mkdirp": "^0.5.1",
    "mocha": "^2.3.5",
    "moment": "^2.12.0",
    "rollup": "^0.25.7",
    "rollup-plugin-commonjs": "^2.2.1",
    "rollup-plugin-json": "^2.0.0",
    "rollup-plugin-node-resolve": "^1.5.0",
    "sift": "^3.2.1"
  },
  "peerDependencies": {
    "aws-sdk": "2.2.x"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:guardian/aws-s3-facia-tool.git"
  },
  "files": [
    "dist"
  ]
}
